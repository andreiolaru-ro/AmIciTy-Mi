				<event.location.y first="0" last="30" count="2" select="1" for-each="time"/>
				<event.domain.a min="0" max="1" for-each="time"/>
				<event.domain.b min="0" max="1" for-each="time"/>
				<event.domain.c min="0" max="1" for-each="time"/>
			</event.time>
		</event>		
	</timeline>
